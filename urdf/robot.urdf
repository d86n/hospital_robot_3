<?xml version="1.0" ?>
<robot name="my-robot">
  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="body"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="body">
    <inertial>
      <origin xyz="-1.3167e-07 -0.0256451 0.365864" rpy="0 0 0"/>
      <mass value="57.976"/>
      <inertia ixx="8.74891" ixy="-6.69946e-07" ixz="-4.47173e-06" iyy="8.88386" iyz="0.454021" izz="1.73978"/>
    </inertial>

    <visual>
      <origin xyz="2.47419e-17 -0.00293702 0.0425" rpy="0 -0 3.14159"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/body.stl"/>
      </geometry>
      <material name="body_material">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="2.47419e-17 -0.00293702 0.0425" rpy="0 -0 3.14159"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/body.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="left_wheel">
    <inertial>
      <origin xyz="4.42896e-18 6.87064e-18 0.00300646" rpy="0 0 0"/>
      <mass value="0.465267"/>
      <inertia ixx="0.00047604" ixy="2.68315e-19" ixz="7.20047e-20" iyy="0.00047604" iyz="-5.32996e-35" izz="0.000800321"/>
    </inertial>

    <visual>
      <origin xyz="0 6.93889e-18 0.01" rpy="-0.391796 1.5708 0"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/standard_wheel.stl"/>
      </geometry>
      <material name="left_wheel_material">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 6.93889e-18 0.01" rpy="-0.391796 1.5708 0"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/standard_wheel.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="left_wheel_joint" type="continuous">
    <origin xyz="-0.23 -0.102937 0.0575" rpy="-1.5708 -0 -1.5708"/>
    <parent link="body"/>
    <child link="left_wheel"/>
    <axis xyz="0 0 -1"/>
    <limit effort="20" velocity="20"/>
  </joint>

  <link name="right_wheel">
    <inertial>
      <origin xyz="2.49079e-18 2.66839e-18 0.00300646" rpy="0 0 0"/>
      <mass value="0.465267"/>
      <inertia ixx="0.00047604" ixy="1.74453e-19" ixz="7.87552e-20" iyy="0.00047604" iyz="-6.1789e-35" izz="0.000800321"/>
    </inertial>

    <visual>
      <origin xyz="0 6.93889e-18 0.01" rpy="0.391796 1.5708 0"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/standard_wheel.stl"/>
      </geometry>
      <material name="right_wheel_material">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 6.93889e-18 0.01" rpy="0.391796 1.5708 0"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/standard_wheel.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="right_wheel_joint" type="continuous">
    <origin xyz="0.23 -0.102937 0.0575" rpy="1.5708 -0 -1.5708"/>
    <parent link="body"/>
    <child link="right_wheel"/>
    <axis xyz="0 0 1"/>
    <limit effort="20" velocity="20"/>
  </joint>

  <link name="spherical_wheel">
    <inertial>
      <origin xyz="-9.7589e-19 -2.2554e-50 -6.93889e-18" rpy="0 0 0"/>
      <mass value="2.48"/>
      <inertia ixx="0.00179354" ixy="-3.5815e-49" ixz="0" iyy="0.00179354" iyz="0" izz="0.00179354"/>
    </inertial>

    <visual>
      <origin xyz="-2.60237e-18 0 -0.0425" rpy="0 -0 -1.38667e-32"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/spherical_wheel.stl"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="-2.60237e-18 0 -0.0425" rpy="0 -0 -1.38667e-32"/>
      <geometry>
        <mesh filename="package://hospital_robot/meshes/spherical_wheel.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_spherical_wheel" type="fixed">
    <origin xyz="2.52012e-17 0.172063 0.0425" rpy="0 -0 2.42861e-16"/>
    <parent link="body"/>
    <child link="spherical_wheel"/>
  </joint>

  <gazebo reference="spherical_wheel">
    <mu1>0.0</mu1>
    <mu2>0.0</mu2>
    <material>Gazebo/Grey</material>
  </gazebo>

  <link name="lidar_link"/>
  <joint name="lidar_joint" type="fixed">
      <parent link="body"/>
      <child link="lidar_link"/>
      <origin xyz="0 0 0.15" rpy="0 0 0"/>
  </joint>

  <gazebo reference="lidar_link">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize> 
      <update_rate>20</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-3.14159</min_angle>
            <max_angle>3.14159</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.35</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/scan</topicName>
        <frameName>lidar_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>20</updateRate>
      
      <leftJoint>left_wheel_joint</leftJoint>
      <rightJoint>right_wheel_joint</rightJoint>
      
      <wheelSeparation>0.46</wheelSeparation> 
      <wheelDiameter>0.2</wheelDiameter>   
      <wheelTorque>20</wheelTorque>
      <wheelAcceleration>1.0</wheelAcceleration>
      
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>base_footprint</robotBaseFrame>
      
      <publishWheelTF>false</publishWheelTF>
      <publishOdomTF>true</publishOdomTF>

      <rosDebugLevel>na</rosDebugLevel>
      <odometrySource>world</odometrySource>
      <publishTf>true</publishTf>
    </plugin>
  </gazebo>

  <gazebo reference="left_wheel">
    <mu1>1.0</mu1> <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <material>Gazebo/DarkGrey</material>
  </gazebo>

  <gazebo reference="right_wheel">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <material>Gazebo/DarkGrey</material>
  </gazebo>
</robot>